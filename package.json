
"name": "SkyCast",
"version": "1.0.0",
"description": "SkyCast - A TypeScript-powered weather app",
"type": "module",
"scripts": {
  "install": "npm install && cd client && npm install",
  "build:client": "npm --prefix client run build",
  "build:server": "npx tsc --project tsconfig.server.json",
  "build": "npm run build:client && npm run build:server",
  "start": "npm run build && node dist/server.js",
  "dev": "nodemon --watch server/src --ext ts,json --exec \"npx tsc --project tsconfig.server.json && node dist/server.js\"",
  "test": "echo \"Error: no test specified\" && exit 1"
},
"author": "",
"license": "ISC",
"dependencies": {
  "dayjs": "^1.11.10",
  "dotenv": "^16.4.5",
  "express": "^4.19.2",
  "uuid": "^9.0.1"
},
"devDependencies": {
  "@types/dotenv": "^8.2.0",
  "@types/express": "^4.17.21",
  "@types/node": "^20.12.7",
  "@types/uuid": "^9.0.8",
  "nodemon": "^3.1.0",
  "typescript": "^5.4.5"
}



